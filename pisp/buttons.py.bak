import RPi.GPIO as GPIO
import time

GPIO.setmode(GPIO.BCM)
button_list = [5, 6, 13, 19, 26, 12, 16, 20]
GPIO.setup(button_list, GPIO.IN, pull_up_down=GPIO.PUD_UP)

def buttonPressed(buttonPin):
	print(buttonPin)


for pin in button_list:
	GPIO.add_event_detect(pin, GPIO.FALLING, callback=buttonPressed, bouncetime=180)

try:
	while True:
		time.sleep(1)
finally:
	GPIO.cleanup()
